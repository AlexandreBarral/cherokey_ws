<launch>
  <node pkg="rosserial_python" type="serial_node.py" name="serial_node"
        output="screen"
        ns="base"  >
    <param name="port" value="/dev/ttyAMA0"/>
    <param name="baud" value="115200"/>
  </node>
  <node
    pkg="cherokey" type="odom_msg_remap" name="odom_msg_remap"
    output="screen"
    ns="base"
  />
</launch>
